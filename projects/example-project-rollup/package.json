{
  "name": "example-project-rollup",
  "version": "1.0.0",
  "private": true,
  "description": "An example project that uses the spectrum web components and gives an example of how to bundle them minimally with rollup.",
  "license": "Apache-2.0",
  "author": "example-rollup-app",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && yarn build:tsc && rollup -c rollup.config.js",
    "build:tsc": "tsc && cp src/styles.css out-tsc/src",
    "start": "concurrently --kill-others --names tsc,wds \"npm run tsc:watch\" \"wds --config wds.config.js\"",
    "start:build": "npm run build && wds --root-dir dist --app-index index.html --open --compatibility none"
  },
  "dependencies": {
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-html": "^1.0.2",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@spectrum-web-components/icon": "0.12.5",
    "@spectrum-web-components/icons": "0.9.5",
    "@spectrum-web-components/icons-ui": "0.9.5",
    "@spectrum-web-components/icons-workflow": "0.9.5",
    "@spectrum-web-components/theme": "0.14.9",
    "@swc-uxp-wrappers/tooltip": "*",
    "lit": "^2.0.0",
    "rollup-plugin-html": "^0.2.1"
  },
  "devDependencies": {
    "@open-wc/building-rollup": "^2.2.1",
    "@open-wc/eslint-config": "^10.0.0",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@types/node": "^18.11.0",
    "@typescript-eslint/eslint-plugin": "^5.35.0",
    "@typescript-eslint/parser": "^5.35.0",
    "@web/dev-server": "^0.2.1",
    "concurrently": "^5.3.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^3.25.1",
    "rollup-plugin-styles": "^4.0.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "tslib": "^2.3.1",
    "typescript": "^4.7.2"
  },
  "customElements": "custom-elements.json",
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  }
}
